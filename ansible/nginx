server {
    listen 80;
    server_name ptony82.dyndns-server.com;
    access_log /var/log/groupvpn-webui/nginx-access.log;
    error_log /var/log/groupvpn-webui/nginx-error.log;

    location / {
        include proxy_params;
        proxy_pass http://localhost:8080;
    }
    location /static/ {
        root /opt/groupvpn-webui;
    }
}
